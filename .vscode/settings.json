{
    "conventionalCommits.promptFooter": false,
    "conventionalCommits.promptBody": false,
    "editor.wordWrap": "on",
    "conventionalCommits.scopes": [
        "deps",
        "stations",
        "cmd",
        "ts",
        "struct",
        "easteregg",
        "ref",
        "event"
    ],
    "tabout.disableByDefault": false,
    "tabout.charactersToTabOutFrom": [
        {
            "open": "[",
            "close": "]"
        },
        {
            "open": "{",
            "close": "}"
        },
        {
            "open": "(",
            "close": ")"
        },
        {
            "open": "'",
            "close": "'"
        },
        {
            "open": "\"",
            "close": "\""
        },
        {
            "open": ":",
            "close": ":"
        },
        {
            "open": "=",
            "close": "="
        },
        {
            "open": ">",
            "close": ">"
        },
        {
            "open": "<",
            "close": "<"
        },
        {
            "open": ".",
            "close": "."
        },
        {
            "open": "`",
            "close": "`"
        },
        {
            "open": ";",
            "close": ";"
        }
    ],
    "conventionalCommits.silentAutoCommit": true,
    "eslint.enable": true,
    "todo-tree.highlights.customHighlight": {
        "BUG": {
            "icon": "bug",
            "iconColour": "#ff0000"
        },
        "HACK": {         
            "icon": "tools",
            "iconColour": "#FFBB00"
        },
        "FIXME": {
            "iconColour": "#0022FF",
            "icon": "file"
        },
        "TODO": {
            "iconColour": "#ff0000"
        }
    }
}
